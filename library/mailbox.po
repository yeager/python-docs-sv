"destination = mailbox.MH('~/Mail')\n"
"destination.lock()\n"
"for message in mailbox.Babyl('~/RMAIL'):\n"
"    destination.add(mailbox.MHMessage(message))\n"
"destination.flush()\n"
"destination.unlock()"
